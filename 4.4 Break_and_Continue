# 1
count = 0

while count <= 9:
    count += 1
    if count == 7:
        continue
    print(count)

# 2
count = 5
total_sum = 0

while count <= 20:
    if count % 3 != 0:
        total_sum = total_sum + count
    if count == 20:
        break
    count += 1

print(total_sum)

#3
starting = int(input("Enter starting number: "))
ending = int(input("Enter ending nubmer: "))
total = 0

while starting <= ending:
    total = total + starting
    starting = starting + 1
    if starting == 13 or starting == 31:
        break

print(total)

# 4
while True:
    word = input("Enter a word: ")
    if word == "stop":
        break
    print(f"Your word: {word}")

print("Goodbye! ")
